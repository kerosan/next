{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 npx prisma generate",
    "prisma:pull": "npx prisma db pull",
    "prisma:push": "npx prisma db push",
    "prisma:migrate:dev": "npx prisma migrate dev --name add_smid",
    "prisma:migrate:reset": "npx prisma migrate reset",
    "prisma:studio": "npx prisma studio",
    "codegen": "graphql-codegen --config src/graphql/codegen.ts"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.0",
    "@ant-design/icons": "^5.4.0",
    "@ant-design/nextjs-registry": "^1.0.1",
    "@apollo/client": "^3.11.4",
    "@apollo/experimental-nextjs-app-support": "^0.11.2",
    "@apollo/server": "^4.11.0",
    "@biomejs/biome": "^1.8.3",
    "@prisma/client": "^5.19.1",
    "@prisma/extension-accelerate": "^1.1.0",
    "antd": "^5.20.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.12",
    "grammy": "^1.29.0",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.7.0",
    "next": "^14.2.6",
    "next-connect": "^1.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "graphql-tag": "^2.12.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
